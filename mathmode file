def list(text):
    l=[]
    for i in text.split(' '):
        try:
            l.append(float(i));
        except ValueError:
            pass
    return l
def add(a,b):
    return a+b
def sub(a,b):
    return a-b
def mul(a,b):
    return a*b
def div(a,b):
    return a/b
